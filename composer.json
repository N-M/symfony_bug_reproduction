{
    "name": "platform",
    "license": "proprietary",
    "type": "project",
    "description": "",
    "autoload": {
        "psr-4": {
            "CoreBundle\\": "src/CoreBundle/"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
    },
    "require": {
        "php": ">=7.1.3",
        "beberlei/DoctrineExtensions": "^1.0",
        "doctrine/dbal": "^2.5",
        "doctrine/doctrine-bundle": "^1.8.1",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "1.3.1",
        "doctrine/orm": "^2.5",
        "gedmo/doctrine-extensions": "^2.4",
        "jms/serializer-bundle": "^2.2",
        "knplabs/knp-paginator-bundle": "^2.7",
        "nelmio/cors-bundle": "^1.5",
        "nelmio/security-bundle": "^2.3",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "^5.0",
        "snc/redis-bundle": "2.x-dev",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/swiftmailer-bundle": "^3.1.5",
        "symfony/symfony": "4.0.11",
        "twig/twig": "^2.0"
    },
    "require-dev": {

    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
